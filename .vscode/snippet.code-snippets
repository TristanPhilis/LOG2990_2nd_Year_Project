{
    // Place your log2990-210 workspace snippets here. Each snippet is defined under a snippet name and has a scope, prefix, body and
    // description. Add comma separated ids of the languages where the snippet is applicable in the scope field. If scope
    // is left empty or omitted, the snippet gets applied to all languages. The prefix is what is
    // used to trigger the snippet and the body will be expanded and inserted. Possible variables are:
    // $1, $2 for tab stops, $0 for the final cursor position, and ${1:label}, ${2:another} for placeholders.
    // Placeholders with the same ids are connected.
    // Example:
    "Print to console": {
        "scope": "javascript,typescript",
        "prefix": "log",
        "body": ["console.log('$1');", "$2"],
        "description": "Log output to console"
    },
    "test it": {
        "scope": "javascript,typescript",
        "prefix": "test",
        "body": ["it('$1', () => {$0});"],
        "description": "test format"
    },

    "no magic number": {
        "scope": "javascript,typescript",
        "prefix": "nomagic",
        "body": ["// tslint:disable-next-line:no-magic-numbers"],
        "description": "disable tslint no magic number rule"
    },

    "no string litteral": {
        "scope": "javascript,typescript",
        "prefix": "nostring",
        "body": ["// tslint:disable-next-line:no-string-literal"],
        "description": "disable tslint no string literal rule"
    },

    "no any ": {
        "scope": "javascript,typescript",
        "prefix": "noany",
        "body": ["// tslint:disable-next-line:no-any"],
        "description": "disable tslint no any rule"
    }
}
